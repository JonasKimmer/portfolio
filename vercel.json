{
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "functions": {
    "server/server.js": {
      "runtime": "@vercel/node"
    }
  }
}
